		  { "type": "alias", "params": [ "tx_crit_upper" ] } ],
		[ { "type": "field", "params": [ "link_tx_throughput_mbps.crit_lower" ] },
		  { "type": "mean",  "params": [] },
		  { "type": "alias", "params": [ "tx_crit_lower" ] } ],
		[ { "type": "field", "params": [ "link_tx_throughput_mbps" ] },
		  { "type": "mean",  "params": [] },
		  { "type": "alias", "params": [ "tx/min" ] } ]
	    ]
	} ],
	"nullPointMode": "connected"
    }, {
	"title": "Network Link \"[%target%]\" Traffic in [%hostname%]",
	"id": [%serial%],
	"datasource": "[%GRAFANADATASOURCE%]",
