//// Generic Volume
{
    "title": "Row",
    "collapse": false,
    "editable": true,
    "height": "250px",
    "panels": [ {
	"title": "[%target%] volume in [%hostname%]",
	"id": [%serial%],
	"datasource": "[%GRAFANADATASOURCE%]",
	"isNew": true,
	"editable": true,
	"tooltip": { "shared": true, "value_type": "individual" },
	"type": "graph",
	"x-axis": true,
	"y-axis": true,
	"y_formats": [ "short" ],
	"grid": { "leftLogBase": 1, "leftMax": null, "leftMin": null },
	"gridPos": { "w": 12, "h": 6, "x": 0, "y": 0 },
	"span": 12,
	"legend": {
	    "show": true,
	    "values": true, "avg": true, "max": true, "min": true, "current": false, "total": false,
	    "rightSide": true, "alignAsTable": true, "sideWidth": 300,
	    "hideEmpty": true, "hideZero": false
	},
	"targets": [ {
	    "refId": "0001",
	    "dsType": "influxdb",
	    "resultFormat": "time_series",
	    "alias": "$col",
	    "groupBy": [ { "type": "time", "params": [ "$__interval" ] } ],
	    "measurement": "[%measurement%]",
	    "tags": [ { "key": "hostname", "operator": "=", "value": "[%hostname%]" } ],
	    "select": [
		[ { "type": "field", "params": [ "[%target%]_total" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "total" ] } ],
		[ { "type": "field", "params": [ "[%target%]_avail" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "avail" ] } ],
		[ { "type": "field", "params": [ "[%target%]_avail" ] },
		  { "type": "min", "params": [] },
		  { "type": "alias", "params": [ "avail_min" ] } ],
		[ { "type": "field", "params": [ "[%target%]_free" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "free" ] } ],
		[ { "type": "field", "params": [ "[%target%]_free" ] },
		  { "type": "min", "params": [] },
		  { "type": "alias", "params": [ "free_min" ] } ],
		[ { "type": "field", "params": [ "[%target%]_used" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "used" ] } ],
		[ { "type": "field", "params": [ "[%target%]_avail.warn_lower" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "warn_lower" ] } ],
		[ { "type": "field", "params": [ "[%target%]_avail.crit_lower" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "crit_lower" ] } ],
		[ { "type": "field", "params": [ "[%target%]_free.warn_lower" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "warn_lower" ] } ],
		[ { "type": "field", "params": [ "[%target%]_free.crit_lower" ] },
		  { "type": "mean", "params": [] },
		  { "type": "alias", "params": [ "crit_lower" ] } ]
	    ]
	} ],
        "nullPointMode": "connected"
    } ]
}
