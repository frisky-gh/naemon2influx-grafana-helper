.*			\w+\.warn_lower		IGNORE
.*			\w+\.crit_upper		IGNORE
.*			\w+\.crit_lower		IGNORE
.*OS_Performance,.*	(\w+)			generic	   generic:$1   target=$1
.*OS_Performance,.*	(\w+_pct)		gpercent   gpercent:$1  target=$1
.*OS_Performance,.*	(\w+)_(total|used|free|avail) gvolume gvolume:$1 target=$1
